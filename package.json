{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db:delete": "dropdb matchgame",
    "db:create": "createdb matchgame",
    "db:schema": "psql -f schema.sql matchgame",
    "db:seed": "psql -f seed.sql matchgame",
    "db:reset": "npm run db:delete; npm run db:create && npm run db:schema && npm run db:seed",
    "dev": "nodemon --ignore './sessions/' ./bin/www"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.21.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg-promise": "^10.4.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
